<proxy xmlns="http://ws.apache.org/ns/synapse" name="OUT_ONLY_TrueTestProxy"
       transports="https http"
       startOnLoad="true"
       trace="disable">
    <description/>
    <target>
        <inSequence>
            <property name="OUT_ONLY" value="true" scope="default" type="STRING"/>
            <send>
                <endpoint>
                    <address uri="http://localhost:9000/services/SimpleStockQuoteService"/>
                </endpoint>
            </send>
        </inSequence>
        <outSequence>
            <respond/>
        </outSequence>
    </target>
</proxy>