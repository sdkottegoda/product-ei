<proxy xmlns="http://ws.apache.org/ns/synapse" name="HEADTestProxy"
       transports="https http"
       startOnLoad="true"
       trace="disable">
    <description/>
    <target>
        <inSequence>
            <log>
                <property name="MSG" value="Head Request"/>
            </log>
            <property name="Allow" value="POST, GET, HEAD" scope="transport"/>
            <respond/>
        </inSequence>
    </target>
</proxy>