<proxy xmlns="http://ws.apache.org/ns/synapse" name="HeadMethodTestProxy"
       transports="https http"
       startOnLoad="true"
       trace="disable">
    <description/>
    <target>
        <inSequence>
            <send>
                <endpoint>
                    <http method="head"
                          uri-template="http://localhost:8480/services/HEADTestProxy"/>
                </endpoint>
            </send>
        </inSequence>
        <outSequence>
            <log level="full"/>
            <send/>
        </outSequence>
    </target>
</proxy>